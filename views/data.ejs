
<!-- ---------------------------------------SELECT OPTION -------------------------->

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/monitor.css">
    <title><%= __('Data') %></title>
    <script src = "js/FC_Common.js"></script>
    <script src = "js/fn_show_plc_data.js"></script>
    <script src = "js/fn_show_import_selector.js"></script>
    <script src = "js/fn_excel_import_selector.js"></script>
    <script src = "socket.io/socket.io.js"></script>
    <script src = "js/jquery-3.6.0.min.js"></script>
    <script src = "js/FileSaver.js"></script> 
     
    
    <script>
        var socket = io()  
    </script>
     
</head>

<body>
    <%- include ('./partials/sidebar') %>
        <div class="cnt_">
             
            <div id="import_selector" class="block_1">
                <div class="thanh_trichxuat_sb_error">
                    <div class="row_flex">  
                        
                        <select id="caseNoSelector" class="searchInput" onchange="fn_import_selector_By_Time();">
                            <option value=""><%= __('Select Case No') %></option>
                        </select>
                        <select id="partNoSelector" class="searchInput" onchange="fn_import_selector_By_Time();">
                            <option value=""><%= __('Select Part No') %></option>
                        </select>

                        <select id="resultSelector" class="searchInput" onchange="fn_import_selector_By_Time();">
                            <option value=""><%= __('Select Result') %></option>
                        </select>

                        <button class="btt_trich_xuat btt_orange" onclick="fn_import_selector_Show();" title="<%= __('Refresh') %> ">
                            <i class="fa-solid fa-arrows-rotate btn_i" aria-hidden="true"></i>
                            
                        </button>   

                        <button class="btt_trich_xuat btt_green" onclick="fn_excel_import_selector_detail();" title="<%= __('Export report') %>">
                            <i class="fa-regular fa-file-excel btn_i" aria-hidden="true"></i>
                        </button>
                    </div>

                    <div class="row_flex">
                        <div id="pagination_box__" class="pagination_box">
                            <button class="btn_page prev" id="prevPage__" onclick="funcprev__()">                               
                                <i class='bx bx-chevron-left'></i>
                            </button>
                            <input id="pageNumberInput__" class="pageNumberInput" min="1" value="1">
                            <button class="btn_page next" id="nextPage__" onclick="funcfor__()">                             
                                <i class='bx bx-chevron-right' ></i>                          
                            </button>
                            <button class="btn_page goto" id="goToPage__" onclick="funcsearch__()">                               
                                <i class='bx bx-chevrons-right' ></i>
                            </button>
                        </div>
                    </div>

                    <div class="row_flex">
                        <div class="xoay">
                            <img id="loadingImage__" class="img_loading1" src="/images/dev_Q/loading.png" />
                        </div>           
                        
                        <button class="btt_trich_xuat btt_green" onclick="fn_excel_import_selector();" title="<%= __('Export report') %>">
                            <i class="fa-regular fa-file-excel btn_i" aria-hidden="true"></i>
                        </button>
                    </div>
             

             </div>
                <div id="div_table_import_selector">
                    <table id="table_import_selector">
                        <thead>
                            <tr>
                                <th >
                                    <%= __('Case No') %>
                                </th>
                                <th >
                                    <%= __('Part No') %>
                                </th>
                                <th >
                                    <%= __('Number of boxes') %>
                                </th>
                                <th >
                                    <%= __('Real') %>
                                </th>
                                <th >
                                    <%= __('Result') %>
                                </th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>


        <script> fn_import_selector_Show();</script>
        <script> fn_import_selector_Show_();</script>
        <script> fn_excel_display_import_selector_detail();</script>
        <script> fn_excel_display_import_selector();</script>

        <script>
            fn_import_selector_By_Time_display();
            fn_case_no();
            fn_part_no(); 
            fn_result_options();             
        </script>





</body>

</html>